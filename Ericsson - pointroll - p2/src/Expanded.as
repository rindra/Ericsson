package  {	import flash.display.Sprite;	import flash.events.MouseEvent;	import com.greensock.TweenMax;	import com.greensock.TimelineLite;	import com.greensock.easing.Quint;	import flash.display.Bitmap;	import flash.display.Shape;	import flash.events.Event;		public class Expanded extends Sprite{				private var track:Function;		private var mc:MC;				public function Expanded() {			// constructor code			addEventListener(Event.ADDED_TO_STAGE, _init);		}				public function _init($e:Event):void{			var auto:Boolean = true;			var count:uint;			var bmp0:Bitmap = new Bitmap(new Bmp0);			var bmp1:Bitmap = new Bitmap(new Bmp1);			var bmp2:Bitmap = new Bitmap(new Bmp2);			var bmp3:Bitmap = new Bitmap(new Bmp3);						mc = new MC;			mc.cta.alpha = 0;			mc.cta.buttonMode = true;			mc.close.alpha = 0;			mc.close.buttonMode = true;			mc.cta.addEventListener(MouseEvent.MOUSE_DOWN, _cta);			mc.close.addEventListener(MouseEvent.MOUSE_DOWN, _close);			mc.nav.buttonMode = true;			mc.nav.addEventListener(MouseEvent.MOUSE_DOWN, _nav);			mc.nav.addEventListener(MouseEvent.MOUSE_OVER, function($e:MouseEvent):void{ $e.target.alpha = 1; });			mc.nav.addEventListener(MouseEvent.MOUSE_OUT, function($e:MouseEvent):void{ $e.target.alpha = .7; });			mc.gal.addChild(bmp0);			mc.gal.addChild(bmp1);			mc.gal.addChild(bmp2);			mc.gal.addChild(bmp3);			addChild(mc);						intro();						function intro():void{				clear();				var t:TimelineLite = new TimelineLite({onComplete:show});				t.insert(TweenMax.from(mc.bg, 1, {alpha:0}));				t.insert(TweenMax.to(mc.gal.getChildAt(0), 1, {alpha:1, y:0, ease:Quint.easeOut}));			}						function _nav($e:MouseEvent):void{				if($e.target.name == 'navRight'){					auto = false;					count = (count+1 < mc.gal.numChildren)?++count:0;					//track click on right arrow				}				if($e.target.name == 'navLeft'){					auto = false;					count = (count-1 >= 0)?--count:mc.gal.numChildren-1;					//track click on left arrow				}				showNav();			}						function show():void{				TweenMax.to(mc, 1, {delay:3, onComplete:function():void{					clear();					if(auto) count = (count+1<mc.gal.numChildren)?count+1:0					TweenMax.to(mc.gal.getChildAt(count), 1, {alpha:1, ease:Quint.easeOut, onComplete:function():void{								if(auto) show();					}});				}});			}			function showNav():void{				TweenMax.killAll();				clear();				TweenMax.to(mc.gal.getChildAt(count), 1, {alpha:1, ease:Quint.easeOut});				//track tab views			}						function clear():void{				for(var i:uint; i<mc.gal.numChildren; i++){					var el:Bitmap = Bitmap(mc.gal.getChildAt(i));					el.alpha = 0;				}			}						function _cta($e:MouseEvent):void{				//exit function			}						function _close($e:MouseEvent):void{				//close ad			}					}	}	}